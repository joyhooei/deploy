{
    "root": "./",
    "title": "通过 GitBook 开源框架和 GitHub 私有化部署 Wiki 文档",
    "gitbook": ">= 3.0.0",

    "language": "zh-hans",

    "plugins": [
        "-sharing",
        "-fontsettings",
        "-lunr",
        "-search",
        "search-pro",
        "theme-official",
        "sitemap-general"
    ],

    "pluginsConfig": {
        "sitemap-general": {
            "prefix": "http://zitiao.org/deploy/"
        }
    }
}
